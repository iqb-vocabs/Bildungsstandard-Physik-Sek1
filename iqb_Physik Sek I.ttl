        <phy001/d56c02a2-6204-4257-80e5-df84b0f119fe>,
        <phy001/e92cda88-6c99-4cf7-b06b-45562b3646f2> ;
    skos:prefLabel "Fachwissen"@de ;
    skos:topConceptOf <> .

