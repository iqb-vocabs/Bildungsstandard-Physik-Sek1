        <phy001/e7e23960-9d74-4e3f-b58f-858af2cdcd8d>,
        <phy001/f30b32be-a17e-42b0-b6e0-7572780db286> ;
    skos:prefLabel "Fachwissen"@de ;
    skos:topConceptOf <> .

